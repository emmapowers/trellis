{
  "name": "trellis-playground",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prebuild": "trellis --app-root ../../trellis-lib bundle --dest ../trellis",
    "build": "esbuild src/main.tsx --bundle --outfile=dist/playground.js --format=esm --platform=browser --target=es2022 --jsx=automatic --loader:.tsx=tsx --loader:.ts=ts --alias:decimal.js-light=decimal.js-light/decimal.mjs",
    "prewatch": "trellis --app-root ../../trellis-lib bundle --dest ../trellis",
    "watch": "esbuild src/main.tsx --bundle --outfile=dist/playground.js --format=esm --platform=browser --target=es2022 --jsx=automatic --loader:.tsx=tsx --loader:.ts=ts --alias:decimal.js-light=decimal.js-light/decimal.mjs --watch"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-aria": "^3.35.0",
    "react-stately": "^3.33.0",
    "@react-aria/toolbar": "^3.0.0-beta.9",
    "uplot": "^1.6.31",
    "recharts": "^3.6.0",
    "lucide-react": "^0.468.0",
    "@msgpack/msgpack": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "esbuild": "^0.27.2",
    "typescript": "^5.7.0"
  }
}
